{
  "name": "@wsvaio/pinia-plugin-persist",
  "type": "module",
  "version": "1.0.0",
  "description": "pinia持久化插件，可兼容uniapp",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/wsvaio",
  "homepage": "https://github.com/wsvaio/pinia-plugin-persist#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/pinia-plugin-persist.git"
  },
  "bugs": {
    "url": "https://github.com/wsvaio/pinia-plugin-persist/issues"
  },
  "keywords": [
    "wsvaio",
    "uniapp",
    "persist",
    "pinia",
    "pinia-plugin",
    "pinia-plugin-persist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "vite ./playground",
    "build": "tsup",
    "commit": "czg",
    "prerelease": "npm run build && npm version prerelease && npm publish",
    "prepatch": "npm run build && npm version prepatch && npm publish",
    "patch": "npm run build && npm version patch && npm publish",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "pinia": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@types/node": "^20.3.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@wsvaio/commitlint-config": "^0.0.6",
    "@wsvaio/eslint-config": "^0.0.6",
    "czg": "^1.6.1",
    "eslint": "^8.42.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
