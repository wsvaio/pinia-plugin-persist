{
  "name": "@wsvaio/pinia-plugin-persist",
  "version": "0.0.4-0",
  "description": "pinia持久化插件，可兼容uniapp",
  "author": "wsvaio <wsvaio@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/wsvaio/pinia-plugin-persist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsvaio/pinia-plugin-persist.git"
  },
  "keywords": [
    "wsvaio",
    "uniapp",
    "persist",
    "pinia",
    "pinia-plugin",
    "pinia-plugin-persist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "prerelease": "npm run build && npm version prerelease && npm publish",
    "patch": "npm run build && npm version patch && npm publish"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.9",
    "rollup": "^3.3.0",
    "rollup-plugin-dts": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@wsvaio/utils": "^0.0.17",
    "pinia": "^2.0.33"
  }
}
